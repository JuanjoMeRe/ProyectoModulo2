@model List<VideoGamesCodexASP.Models.Videogames>
@{
    ViewBag.Title = "Videogame Codex";
}

<h1>Ranking</h1>

<table class="table table-striped table-inverse">
    <thead>
        <tr>
            <th>Name</th>
            <th>Score</th>
            

        </tr>
    </thead>
    <tbody>
        @foreach (VideoGamesCodexASP.Models.Videogames ranking in Model)
        {
            <tr>

                <td>@ranking.name</td>
                <td>@ranking.score</td>
             
                             
            </tr>

        }

    </tbody>
</table>